{
  "extends": "../../tsconfig.json",
  "include": ["./src/**/*.ts", "./src/**/*.tsx"],
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": ".",
    "baseUrl": ".",
    "lib": ["DOM", "DOM.Iterable", "es2023"],
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "incremental": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "jsx": "react-jsx"
  }
}
